{
  "name": "diet-tracker-back",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose -f docker-compose.dev.yml up --build app",
    "start:update": "docker-compose -f docker-compose.dev.yml up --build -V app",
    "start:dev": "nodemon src/index.ts",
    "start:prod": "node ./build/index.js",
    "build": "tsc -p .",
    "docker:build": "docker build --build-arg --target=production -t diet-tracker-back .",
    "docker:push": "docker tag diet-tracker-back jetpackpony/diet-tracker-back && docker push jetpackpony/diet-tracker-back",
    "backup:prod": "node ./build/backup/backupCron.js",
    "backupOnce:prod": "node ./build/backup/backupOnce.js",
    "restore:prod": "node ./build/backup/restore.js",
    "createOrUpdateUser:prod": "node ./build/scripts/createOrUpdateUser.js",
    "migrationAddUserIDsToRecords:prod": "node ./build/scripts/migrationAddUserIDsToRecords.js",
    "backup:dev": "node --loader ts-node/esm/transpile-only src/backup/backupCron.ts",
    "backupOnce:dev": "node --loader ts-node/esm/transpile-only src/backup/backupOnce.ts",
    "restore:dev": "node --loader ts-node/esm/transpile-only src/backup/restore.ts",
    "createOrUpdateUser:dev": "node --loader ts-node/esm/transpile-only src/scripts/createOrUpdateUser.ts",
    "migrationAddUserIDsToRecords:dev": "node --loader ts-node/esm/transpile-only src/scripts/migrationAddUserIDsToRecords.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "@googleapis/drive": "^3.0.1",
    "apollo-server": "^3.9.0",
    "commander": "^11.1.0",
    "escape-string-regexp": "^5.0.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongodb": "^4.7.0",
    "nexus": "^1.3.0",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.3",
    "@types/node-cron": "^3.0.8",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
